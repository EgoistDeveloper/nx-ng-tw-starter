{
  "implicitDependencies": {
    "angular.json": "*",
    "nx.json": "*",
    "package.json": "*",
    "tsconfig.json": "*",
    "tslint.json": "*"
  },
  "npmScope": "nx-ng-starter",
  "projects": {
    "api": {
      "tags": ["scope:api", "type:application", "platform:desktop"]
    },
    "api-interface": {
      "tags": ["scope:api", "type:data-access", "platform:desktop"]
    },
    "mocks-core": {
      "tags": ["scope:mocks-core", "type:feature", "platform:desktop"]
    },
    "nx-ng-starter": {
      "tags": ["scope:nx-ng-starter", "type:application", "platform:desktop"]
    },
    "nx-ng-starter-e2e": {
      "implicitDependencies": ["nx-ng-starter"],
      "tags": []
    },
    "proto": {
      "tags": ["scope:shared", "type:data-access"]
    },
    "shared-core": {
      "tags": ["scope:nx-ng-starter", "type:feature", "platform:desktop"]
    },
    "shared-store": {
      "tags": ["scope:nx-ng-starter", "type:data-access", "platform:desktop"]
    },
    "shared-util": {
      "tags": ["scope:nx-ng-starter", "type:util", "platform:desktop"]
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"],
        "runtimeCacheInputs": ["node -v", "echo $IMPORTANT_ENV_VAR"],
        "cacheDirectory": "/tmp/nx/nx-ng-starter"
      }
    }
  }
}
