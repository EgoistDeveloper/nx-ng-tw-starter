{
  "implicitDependencies": {
    "angular.json": "*",
    "nx.json": "*",
    "package.json": "*",
    "tsconfig.json": "*",
    "tslint.json": "*"
  },
  "npmScope": "nx-ng-starter",
  "projects": {
    "api": {
      "tags": ["scope:api", "type:application"]
    },
    "api-interface": {
      "tags": ["scope:api-interface", "type:data-access"]
    },
    "mocks-core": {
      "tags": ["scope:mocks-core", "type:feature"]
    },
    "nx-ng-starter": {
      "tags": ["scope:nx-ng-starter", "type:application"]
    },
    "nx-ng-starter-e2e": {
      "implicitDependencies": ["nx-ng-starter"],
      "tags": ["scope:nx-ng-starter-e2e"]
    },
    "proto": {
      "tags": ["scope:proto", "type:data-access"]
    },
    "shared-core": {
      "tags": ["scope:shared-core", "type:feature"]
    },
    "shared-store": {
      "tags": ["scope:shared-store", "type:data-access"]
    },
    "shared-ui": {
      "tags": ["scope:shared-ui", "type:ui"]
    },
    "shared-util": {
      "tags": ["scope:shared-util", "type:util"]
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "options": {
        "cacheDirectory": "/tmp/nx/nx-ng-starter",
        "cacheableOperations": ["build", "lint", "test", "e2e"],
        "runtimeCacheInputs": ["node -v", "echo $IMPORTANT_ENV_VAR"]
      },
      "runner": "@nrwl/workspace/tasks-runners/default"
    }
  }
}
