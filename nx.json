{
  "implicitDependencies": {
    "angular.json": "*",
    "nx.json": "*",
    "package.json": "*",
    "tslint.json": "*",
    "tsconfig.base.json": "*"
  },
  "npmScope": "nx-ng-starter",
  "projects": {
    "api": {
      "tags": ["scope:api", "type:application"]
    },
    "api-interface": {
      "tags": ["scope:api-interface", "type:data-access"]
    },
    "mocks-core": {
      "tags": ["scope:mocks-core", "type:mocks"]
    },
    "nx-ng-starter": {
      "tags": ["scope:nx-ng-starter", "type:application"]
    },
    "nx-ng-starter-e2e": {
      "implicitDependencies": ["nx-ng-starter"],
      "tags": ["scope:nx-ng-starter-e2e"]
    },
    "proto": {
      "tags": ["scope:proto", "type:data-access"]
    },
    "client-core": {
      "tags": ["scope:client-core", "type:feature"]
    },
    "client-store": {
      "tags": ["scope:client-store", "type:data-access"]
    },
    "client-ui": {
      "tags": ["scope:client-ui", "type:ui"]
    },
    "client-util": {
      "tags": ["scope:client-util", "type:util"]
    },
    "client-ui-material": {
      "tags": ["scope:client-ui-material", "type:ui"]
    },
    "client-ui-translate": {
      "tags": ["scope:client-ui-translate", "type:ui"]
    },
    "client-services": {
      "tags": ["scope:client-services", "type:ui"]
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "options": {
        "cacheDirectory": "/tmp/nx/nx-ng-starter",
        "cacheableOperations": ["build", "lint", "test", "e2e"],
        "runtimeCacheInputs": ["node -v", "echo $IMPORTANT_ENV_VAR"]
      },
      "runner": "@nrwl/workspace/tasks-runners/default"
    }
  },
  "affected": {
    "defaultBase": "master"
  }
}
