{
  "name": "nx-ng-starter",
  "version": "1.0.0",
  "private": true,
  "description": "NX Angular Starter",
  "license": "MIT",
  "scripts": {
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "api-interface-gql:generate": "ng run api-interface:generate-client-definitions",
    "build:all": "npm run build:api && npm run build:nx-ng-starter",
    "build:all:prod": "bash tools/shell/build.sh prod all doc",
    "build:api": "ng build api",
    "build:api:prod": "bash tools/shell/build.sh prod api",
    "build:nx-ng-starter": "ng build nx-ng-starter",
    "build:nx-ng-starter:prod": "bash tools/shell/build.sh prod nx-ng-starter",
    "changelog:all:generate": "bash tools/shell/changelog.sh all",
    "dep-graph": "nx dep-graph",
    "document": "bash tools/shell/document.sh",
    "document:all:generate": "bash tools/shell/document.sh generate all",
    "document:all:generate-and-report-to-dist": "bash tools/shell/document.sh generate-and-report all",
    "e2e": "bash tools/shell/e2e.sh all",
    "e2e:headless": "bash tools/shell/e2e.sh all headless",
    "e2e:headless:report": "bash tools/shell/e2e.sh all headless report",
    "e2e:nx-ng-starter": "bash tools/shell/e2e.sh app:nx-ng-starter-e2e",
    "e2e:nx-ng-starter:headless": "bash tools/shell/e2e.sh app:nx-ng-starter-e2e headless",
    "e2e:nx-ng-starter:headless:report": "bash tools/shell/e2e.sh app:nx-ng-starter-e2e headless report",
    "e2e:nx-ng-starter:report": "bash tools/shell/e2e.sh app:nx-ng-starter-e2e browser report",
    "firebase:deploy:ci": "bash tools/shell/firebase-deploy.sh",
    "firebase:deploy:localhost:all": "npm run build:all:prod && bash tools/shell/firebase-deploy.sh localhost all",
    "firebase:deploy:localhost:api": "npm run build:api:prod && bash tools/shell/firebase-deploy.sh localhost app:api",
    "firebase:deploy:localhost:nx-ng-starter": "npm run build:nx-ng-starter:prod && bash tools/shell/firebase-deploy.sh localhost app:nx-ng-starter",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "generate:protobufjs": "bash tools/shell/generate-proto.sh protobufjs",
    "generate:protoc": "bash tools/shell/generate-proto.sh protoc",
    "help": "nx help",
    "preinstall": "rm -rf node_modules",
    "postinstall": "npx sort-package-json && npm run ngcc",
    "install:all:linux": "bash tools/shell/install.sh all",
    "install:all:osx": "bash tools/shell/install.sh all osx",
    "install:global": "bash tools/shell/install.sh global",
    "install:proto:linux": "bash tools/shell/install.sh proto",
    "install:proto:osx": "bash tools/shell/install.sh proto osx",
    "install:shellcheck:linux": "bash tools/shell/install.sh shellcheck",
    "install:shellcheck:osx": "bash tools/shell/install.sh shellcheck osx",
    "lint": "npx nx workspace-lint && ng lint",
    "lint:all": "bash tools/shell/lint.sh all",
    "lint:all:fix": "bash tools/shell/lint.sh all fix",
    "lint:changed": "bash tools/shell/lint.sh changed",
    "lint:changed:fix": "bash tools/shell/lint.sh changed fix",
    "ng": "ng",
    "ng-build": "ng build",
    "ng-e2e": "ng e2e",
    "ngcc": "ngcc",
    "nx": "nx",
    "nx:lint": "./node_modules/.bin/nx workspace-lint && ng lint",
    "nx:lint:fix": "./node_modules/.bin/nx workspace-lint && ng lint --fix",
    "prettier:all:html": "npm run prettier:apps:html && prettier:libs:html",
    "prettier:all:html:fix": "npm run prettier:apps:html:fix && prettier:libs:html:fix",
    "prettier:apps:html": "npx prettier -c apps/**/**/*.html",
    "prettier:apps:html:fix": "npx prettier -c --write apps/**/**/*.html",
    "prettier:libs:html": "npx prettier -c libs/**/**/*.html",
    "prettier:libs:html:fix": "npx prettier -c --write libs/**/**/*.html",
    "protolint": "protolint lint -config_path ./.protolint.yaml ./tools/proto/*",
    "shellcheck": "shellcheck **/*.sh",
    "start": "npm run start:api & npm run start:nx-ng-starter",
    "start:api": "ng serve --project api",
    "start:nx-ng-starter": "ng serve --project nx-ng-starter",
    "start:production-server": "node simple-production-server.js",
    "stylelint": "npx stylelint --syntax scss",
    "stylelint:all": "npm run stylelint:apps; npm run stylelint:libs",
    "stylelint:all:fix": "npm run stylelint:apps:fix; npm run stylelint:libs:fix",
    "stylelint:apps": "npx stylelint \"./apps/**/*.scss\"",
    "stylelint:apps:fix": "npx stylelint \"./apps/**/*.scss\" --fix",
    "stylelint:libs": "npx stylelint \"./libs/**/*.scss\"",
    "stylelint:libs:fix": "npx stylelint \"./libs/**/*.scss\" --fix",
    "test": "bash tools/shell/test.sh",
    "test:all:single-run": "bash tools/shell/test.sh single-run all",
    "test:all:single-run-and-report-to-dist": "bash tools/shell/test.sh single-run-and-report all",
    "test:changed:single-run": "bash tools/shell/test.sh single-run changed",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:changed"
    }
  },
