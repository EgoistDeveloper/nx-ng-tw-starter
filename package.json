{
  "name": "nx-ng-starter",
  "version": "1.0.0",
  "private": true,
  "description": "NX Angular Starter",
  "license": "MIT",
  "scripts": {
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "build": "npm run build:api:prod && npm run build:nx-ng-starter:prod",
    "build:api:prod": "ng build api --prod",
    "build:nx-ng-starter:prod": "ng build --prod",
    "dep-graph": "nx dep-graph",
    "e2e": "bash shell/e2e.sh all",
    "e2e:headless": "bash shell/e2e.sh all headless",
    "firebase:deploy:ci": "bash shell/firebase-deploy.sh",
    "firebase:deploy:localhost:all": "npm run build && bash shell/firebase-deploy.sh localhost all",
    "firebase:deploy:localhost:api": "npm run build:api:prod && bash shell/firebase-deploy.sh localhost api",
    "firebase:deploy:localhost:nx-ng-starter": "npm run build:api:nx-ng-starter && bash shell/firebase-deploy.sh localhost nx-ng-starter",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "help": "nx help",
    "postinstall": "npx sort-package-json",
    "lint": "npx nx workspace-lint && ng lint",
    "lint:all": "bash shell/lint.sh all",
    "lint:all:fix": "bash shell/lint.sh all fix",
    "ng": "ng",
    "nx": "nx",
    "prettier:all:html": "npm run prettier:apps:html && prettier:libs:html",
    "prettier:all:html:fix": "npm run prettier:apps:html:fix && prettier:libs:html:fix",
    "prettier:apps:html": "npx prettier -c apps/**/**/*.html",
    "prettier:apps:html:fix": "npx prettier -c --write apps/**/**/*.html",
    "prettier:libs:html": "npx prettier -c libs/**/**/*.html",
    "prettier:libs:html:fix": "npx prettier -c --write libs/**/**/*.html",
    "start": "npm run start:api & npm run start:nx-ng-starter",
    "start:api": "ng serve --project api",
    "start:nx-ng-starter": "ng serve --project nx-ng-starter",
    "start:production-server": "node simple-production-server.js",
    "stylelint": "npx stylelint --syntax scss",
    "stylelint:all": "npm run stylelint:apps; npm run stylelint:libs",
    "stylelint:all:fix": "npm run stylelint:apps:fix; npm run stylelint:libs:fix",
    "stylelint:apps": "npx stylelint \"./apps/**/*.scss\"",
    "stylelint:apps:fix": "npx stylelint \"./apps/**/*.scss\" --fix",
    "stylelint:libs": "npx stylelint \"./libs/**/*.scss\"",
    "stylelint:libs:fix": "npx stylelint \"./libs/**/*.scss\" --fix",
    "test": "bash shell/test.sh",
    "test:all:single-run": "bash shell/test.sh single-run all",
    "test:all:single-run-and-report-to-dist": "bash shell/test.sh single-run-and-report all",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:all"
    }
  },
  "dependencies": {
    "@angular-devkit/build-angular": "0.803.19",
    "@angular/animations": "8.2.14",
    "@angular/cdk": "8.2.3",
    "@angular/cli": "8.3.19",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/compiler-cli": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/elements": "8.2.14",
    "@angular/flex-layout": "8.0.0-beta.26",
    "@angular/forms": "8.2.14",
    "@angular/http": "7.2.15",
    "@angular/language-service": "8.2.14",
    "@angular/material": "8.2.3",
    "@angular/material-moment-adapter": "8.2.3",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/pwa": "^0.803.19",
    "@angular/router": "8.2.14",
    "@angular/service-worker": "8.2.14",
    "@nestjs/common": "^6.10.1",
    "@nestjs/core": "^6.10.1",
    "@nestjs/graphql": "6.5.3",
    "@nestjs/platform-express": "^6.10.1",
    "@nestjs/schematics": "^6.7.3",
    "@nestjs/testing": "^6.10.1",
    "@ngx-translate/core": "11.0.1",
    "apollo-angular": "1.8.0",
    "apollo-angular-link-http": "1.9.0",
    "apollo-cache-inmemory": "1.6.3",
    "apollo-client": "2.6.4",
    "apollo-link": "1.2.13",
    "apollo-link-error": "1.1.12",
    "apollo-server-express": "2.9.9",
    "apollo-upload-client": "11.0.0",
    "apollo-utilities": "1.3.2",
    "class-transformer": "0.2.3",
    "class-validator": "0.11.0",
    "cypress": "3.6.1",
    "dotenv": "8.2.0",
    "graphql": "14.5.8",
    "graphql-subscriptions": "1.1.0",
    "graphql-tag": "2.10.1",
    "graphql-tools": "4.0.6",
    "graphql-upload": "^8.1.0",
    "hammerjs": "2.0.8",
    "isomorphic-fetch": "2.2.1",
    "jest": "24.9.0",
    "jest-preset-angular": "8.0.0",
    "jwt-simple": "0.5.6",
    "marked": "0.7.0",
    "material-design-icon-fonts": "3.0.1",
    "moment": "2.24.0",
    "ngx-take-until-destroy": "5.4.0",
    "reflect-metadata": "^0.1.13",
    "roboto-fontface": "0.10.0",
    "rxjs": "6.5.3",
    "type-graphql": "0.17.5",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@ngxs/devtools-plugin": "^3.5.1",
    "@ngxs/form-plugin": "^3.5.1",
    "@ngxs/logger-plugin": "^3.5.1",
    "@ngxs/store": "^3.5.1",
    "@nrwl/angular": "8.8.0",
    "@nrwl/cypress": "8.8.0",
    "@nrwl/jest": "8.8.0",
    "@nrwl/nest": "8.8.0",
    "@nrwl/node": "8.8.0",
    "@nrwl/workspace": "8.8.0",
    "@types/graphql": "14.5.0",
    "@types/jest": "24.0.23",
    "@types/jquery": "3.3.31",
    "@types/node": "12.12.11",
    "codelyzer": "5.2.0",
    "core-js": "3.4.1",
    "firebase": "7.4.0",
    "firebase-admin": "8.8.0",
    "firebase-functions": "3.3.0",
    "firebase-tools": "7.8.0",
    "husky": "^3.1.0",
    "prettier": "1.19.1",
    "sort-package-json": "1.23.1",
    "stylelint": "^12.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "ts-jest": "24.1.0",
    "ts-node": "8.5.2",
    "tslint": "5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-etc": "^1.9.2",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "3.5.3"
  },
  "engines": {
    "node": ">=10.16.0",
    "npm": ">=6.9.0"
  }
}
