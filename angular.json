{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    }
  },
  "defaultProject": "nx-ng-starter",
  "newProjectRoot": "",
  "projects": {
    "api": {
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "configurations": {
            "production": {
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ],
              "inspect": false,
              "optimization": true
            },
            "firebase": {
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.firebase.ts"
                }
              ],
              "inspect": false,
              "optimization": true
            }
          },
          "options": {
            "assets": ["apps/api/src/assets"],
            "main": "apps/api/src/main.ts",
            "outputPath": "dist/apps/api",
            "tsConfig": "apps/api/tsconfig.app.json",
            "buildLibsFromSource": true
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "apps/api/.eslintrc",
            "exclude": ["**/node_modules/**", "!apps/api/**"],
            "tsConfig": ["apps/api/tsconfig.app.json", "apps/api/tsconfig.spec.json"]
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "configurations": {
            "production": {
              "buildTarget": "api:build:production"
            },
            "firebase": {
              "buildTarget": "api:build:firebase"
            }
          },
          "options": {
            "buildTarget": "api:build"
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true,
            "tsConfig": "apps/api/tsconfig.spec.json"
          }
        }
      },
      "prefix": "api",
      "projectType": "application",
      "root": "apps/api",
      "schematics": {},
      "sourceRoot": "apps/api/src"
    },
    "api-interface": {
      "architect": {
        "generate-client-definitions": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "npx graphql-codegen --config libs/api-interface/codegen.yml && find libs/api-interface/src/lib/generated/ -type f -name \"*.ts\" -exec sed -i \"s/Scalars/IScalars/g\" {} + && ng lint api-interface --fix"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "libs/api-interface/.eslintrc",
            "exclude": ["**/node_modules/**", "!libs/api-interface/**"],
            "tsConfig": [
              "libs/api-interface/tsconfig.lib.json",
              "libs/api-interface/tsconfig.spec.json"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/api-interface/jest.config.js",
            "passWithNoTests": true,
            "tsConfig": "libs/api-interface/tsconfig.spec.json"
          }
        }
      },
      "projectType": "library",
      "root": "libs/api-interface",
      "schematics": {},
      "sourceRoot": "libs/api-interface/src"
    },
    "mocks-core": {
      "architect": {
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "libs/mocks-core/.eslintrc",
            "exclude": ["**/node_modules/**", "!libs/mocks-core/**"],
            "tsConfig": ["libs/mocks-core/tsconfig.lib.json", "libs/mocks-core/tsconfig.spec.json"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/mocks-core/jest.config.js",
            "passWithNoTests": true,
            "setupFile": "libs/mocks-core/src/test-setup.ts",
            "tsConfig": "libs/mocks-core/tsconfig.spec.json"
          }
        }
      },
      "prefix": "nx-ng-starter",
      "projectType": "library",
      "root": "libs/mocks-core",
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      },
      "sourceRoot": "libs/mocks-core/src"
    },
    "nx-ng-starter": {
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "configurations": {
            "analyze-bundle": {
              "aot": true,
              "budgets": [
                {
                  "maximumError": "5mb",
                  "maximumWarning": "2mb",
                  "type": "initial"
                },
                {
                  "maximumError": "10kb",
                  "maximumWarning": "6kb",
                  "type": "anyComponentStyle"
                }
              ],
              "buildOptimizer": true,
              "extractCss": true,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "apps/nx-ng-starter/src/environments/environment.ts",
                  "with": "apps/nx-ng-starter/src/environments/environment.prod.ts"
                }
              ],
              "namedChunks": true,
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "vendorChunk": true
            },
            "production": {
              "aot": true,
              "budgets": [
                {
                  "maximumError": "5mb",
                  "maximumWarning": "2mb",
                  "type": "initial"
                },
                {
                  "maximumError": "10kb",
                  "maximumWarning": "6kb",
                  "type": "anyComponentStyle"
                }
              ],
              "buildOptimizer": true,
              "extractCss": true,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "apps/nx-ng-starter/src/environments/environment.ts",
                  "with": "apps/nx-ng-starter/src/environments/environment.prod.ts"
                }
              ],
              "namedChunks": true,
              "ngswConfigPath": "apps/nx-ng-starter/ngsw-config.json",
              "optimization": true,
              "outputHashing": "all",
              "serviceWorker": true,
              "sourceMap": false,
              "vendorChunk": true
            }
          },
          "options": {
            "aot": true,
            "assets": [
              "apps/nx-ng-starter/src/favicon.ico",
              "apps/nx-ng-starter/src/assets",
              "apps/nx-ng-starter/src/manifest.webmanifest",
              {
                "glob": "**/*",
                "input": "./libs/shared-core/src/lib/assets",
                "output": "./assets"
              }
            ],
            "es5BrowserSupport": true,
            "index": "apps/nx-ng-starter/src/index.html",
            "main": "apps/nx-ng-starter/src/main.ts",
            "outputPath": "dist/apps/nx-ng-starter",
            "polyfills": "apps/nx-ng-starter/src/polyfills.ts",
            "scripts": [],
            "styles": [
              "node_modules/roboto-fontface/css/roboto/roboto-fontface.css",
              "node_modules/material-design-icon-fonts/iconfont/material-icons.css",
              "node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "apps/nx-ng-starter/src/styles.scss"
            ],
            "tsConfig": "apps/nx-ng-starter/tsconfig.app.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "nx-ng-starter:build"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "apps/nx-ng-starter/.eslintrc",
            "exclude": ["**/node_modules/**", "!apps/nx-ng-starter/**"],
            "tsConfig": [
              "apps/nx-ng-starter/tsconfig.app.json",
              "apps/nx-ng-starter/tsconfig.spec.json"
            ]
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "nx-ng-starter:build:production"
            }
          },
          "options": {
            "browserTarget": "nx-ng-starter:build",
            "proxyConfig": "apps/nx-ng-starter/proxy.conf.json"
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "assets": [
              "apps/nx-ng-starter/src/favicon.ico",
              "apps/nx-ng-starter/src/assets",
              {
                "glob": "**/*",
                "input": "./libs/shared-core/src/lib/assets",
                "output": "./assets"
              }
            ],
            "jestConfig": "apps/nx-ng-starter/jest.config.js",
            "passWithNoTests": true,
            "scripts": [],
            "setupFile": "apps/nx-ng-starter/src/test-setup.ts",
            "styles": [
              "node_modules/roboto-fontface/css/roboto/roboto-fontface.css",
              "node_modules/material-design-icon-fonts/iconfont/material-icons.css",
              "node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "apps/nx-ng-starter/src/styles.scss"
            ],
            "tsConfig": "apps/nx-ng-starter/tsconfig.spec.json"
          }
        }
      },
      "prefix": "nx-ng-starter",
      "projectType": "application",
      "root": "apps/nx-ng-starter",
      "schematics": {
        "@nrwl/workspace:component": {
          "style": "scss"
        }
      },
      "sourceRoot": "apps/nx-ng-starter/src"
    },
    "nx-ng-starter-e2e": {
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "configurations": {
            "production": {
              "devServerTarget": "nx-ng-starter:serve:production"
            }
          },
          "options": {
            "cypressConfig": "apps/nx-ng-starter-e2e/cypress.json",
            "devServerTarget": "nx-ng-starter:serve",
            "tsConfig": "apps/nx-ng-starter-e2e/tsconfig.e2e.json"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "apps/nx-ng-starter-e2e/.eslintrc",
            "exclude": ["**/node_modules/**", "!apps/nx-ng-starter-e2e/**"],
            "tsConfig": "apps/nx-ng-starter-e2e/tsconfig.e2e.json"
          }
        }
      },
      "projectType": "application",
      "root": "apps/nx-ng-starter-e2e",
      "sourceRoot": "apps/nx-ng-starter-e2e/src"
    },
    "proto": {
      "architect": {
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "libs/proto/.eslintrc",
            "exclude": ["**/node_modules/**", "!libs/proto/**"],
            "tsConfig": ["libs/proto/tsconfig.lib.json", "libs/proto/tsconfig.spec.json"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/proto/jest.config.js",
            "passWithNoTests": true,
            "setupFile": "libs/proto/src/test-setup.ts",
            "tsConfig": "libs/proto/tsconfig.spec.json"
          }
        }
      },
      "prefix": "nx-ng-starter",
      "projectType": "library",
      "root": "libs/proto",
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      },
      "sourceRoot": "libs/proto/src"
    },
    "shared-core": {
      "architect": {
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "libs/shared-core/.eslintrc",
            "exclude": ["**/node_modules/**", "!libs/shared-core/**"],
            "tsConfig": [
              "libs/shared-core/tsconfig.lib.json",
              "libs/shared-core/tsconfig.spec.json"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/shared-core/jest.config.js",
            "passWithNoTests": true,
            "setupFile": "libs/shared-core/src/test-setup.ts",
            "tsConfig": "libs/shared-core/tsconfig.spec.json"
          }
        }
      },
      "prefix": "nx-ng-starter",
      "projectType": "library",
      "root": "libs/shared-core",
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      },
      "sourceRoot": "libs/shared-core/src"
    }
  },
  "schematics": {
    "@nrwl/angular:application": {
      "e2eTestRunner": "cypress",
      "unitTestRunner": "jest",
      "style": "scss"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest",
      "style": "scss"
    },
    "@nrwl/schematics:application": {
      "framework": "angular",
      "style": "scss"
    },
    "@nrwl/schematics:library": {
      "unitTestRunner": "jest",
      "style": "scss"
    }
  },
  "version": 1
}
