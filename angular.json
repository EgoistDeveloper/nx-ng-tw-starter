{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "warnings": {
      "versionMismatch": false
    }
  },
  "defaultProject": "client",
  "newProjectRoot": "",
  "projects": {
    "api": {
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "configurations": {
            "firebase": {
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.firebase.ts"
                }
              ],
              "inspect": false,
              "optimization": true
            },
            "production": {
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ],
              "inspect": false,
              "optimization": true
            }
          },
          "options": {
            "assets": ["apps/api/src/assets"],
            "buildLibsFromSource": true,
            "main": "apps/api/src/main.ts",
            "outputPath": "dist/apps/api",
            "tsConfig": "apps/api/tsconfig.app.json"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "apps/api/.eslintrc",
            "exclude": ["**/node_modules/**", "!apps/api/**"],
            "tsConfig": ["apps/api/tsconfig.app.json", "apps/api/tsconfig.spec.json"]
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "configurations": {
            "firebase": {
              "buildTarget": "api:build:firebase"
            },
            "production": {
              "buildTarget": "api:build:production"
            }
          },
          "options": {
            "buildTarget": "api:build"
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "prefix": "api",
      "projectType": "application",
      "root": "apps/api",
      "schematics": {},
      "sourceRoot": "apps/api/src"
    },
    "api-interface": {
      "architect": {
        "generate-client-definitions": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "npx graphql-codegen --config libs/api-interface/codegen.yml && find libs/api-interface/src/lib/generated/ -type f -name \"*.ts\" -exec sed -i \"s/Scalars/IScalars/g\" {} + && ng lint api-interface --fix"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "libs/api-interface/.eslintrc",
            "exclude": ["**/node_modules/**", "!libs/api-interface/**"],
            "tsConfig": [
              "libs/api-interface/tsconfig.lib.json",
              "libs/api-interface/tsconfig.spec.json"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/api-interface/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "projectType": "library",
      "root": "libs/api-interface",
      "schematics": {},
      "sourceRoot": "libs/api-interface/src"
    },
    "mocks-core": {
      "architect": {
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "libs/mocks-core/.eslintrc",
            "exclude": ["**/node_modules/**", "!libs/mocks-core/**"],
            "tsConfig": ["libs/mocks-core/tsconfig.lib.json", "libs/mocks-core/tsconfig.spec.json"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/mocks-core/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "prefix": "client",
      "projectType": "library",
      "root": "libs/mocks-core",
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      },
      "sourceRoot": "libs/mocks-core/src"
    },
    "client": {
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "configurations": {
            "analyze-bundle": {
              "aot": true,
              "budgets": [
                {
                  "maximumError": "5mb",
                  "maximumWarning": "2mb",
                  "type": "initial"
                },
                {
                  "maximumError": "10kb",
                  "maximumWarning": "6kb",
                  "type": "anyComponentStyle"
                }
              ],
              "buildOptimizer": true,
              "extractCss": true,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "apps/client/src/environments/environment.ts",
                  "with": "apps/client/src/environments/environment.prod.ts"
                }
              ],
              "namedChunks": true,
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "vendorChunk": true
            },
            "production": {
              "aot": true,
              "budgets": [
                {
                  "maximumError": "5mb",
                  "maximumWarning": "2mb",
                  "type": "initial"
                },
                {
                  "maximumError": "10kb",
                  "maximumWarning": "6kb",
                  "type": "anyComponentStyle"
                }
              ],
              "buildOptimizer": true,
              "extractCss": true,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "apps/client/src/environments/environment.ts",
                  "with": "apps/client/src/environments/environment.prod.ts"
                }
              ],
              "namedChunks": true,
              "ngswConfigPath": "apps/client/ngsw-config.json",
              "optimization": true,
              "outputHashing": "all",
              "serviceWorker": true,
              "sourceMap": false,
              "vendorChunk": true
            }
          },
          "options": {
            "allowedCommonJsDependencies": [
              "node_modules/hammerjs/hammer.js",
              "google-protobuf/google/protobuf/any_pb.js",
              "protobufjs/minimal",
              "zen-observable",
              "apollo-upload-client",
              "extract-files/public/isExtractableFile",
              "extract-files/public/extractFiles"
            ],
            "aot": true,
            "assets": [
              "apps/client/src/favicon.ico",
              "apps/client/src/assets",
              "apps/client/src/manifest.webmanifest",
              {
                "glob": "**/*",
                "input": "./libs/client-core/src/lib/assets",
                "output": "./assets"
              }
            ],
            "index": "apps/client/src/index.html",
            "main": "apps/client/src/main.ts",
            "outputPath": "dist/apps/client",
            "polyfills": "apps/client/src/polyfills.ts",
            "scripts": [],
            "styles": [
              "node_modules/roboto-fontface/css/roboto/roboto-fontface.css",
              "node_modules/material-design-icon-fonts/iconfont/material-icons.css",
              "node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "apps/client/src/styles.scss"
            ],
            "tsConfig": "apps/client/tsconfig.app.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "client:build"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "apps/client/.eslintrc",
            "exclude": ["**/node_modules/**", "!apps/client/**"],
            "tsConfig": ["apps/client/tsconfig.app.json", "apps/client/tsconfig.spec.json"]
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "client:build:production"
            }
          },
          "options": {
            "browserTarget": "client:build",
            "proxyConfig": "apps/client/proxy.conf.json"
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "assets": [
              "apps/client/src/favicon.ico",
              "apps/client/src/assets",
              {
                "glob": "**/*",
                "input": "./libs/client-core/src/lib/assets",
                "output": "./assets"
              }
            ],
            "jestConfig": "apps/client/jest.config.js",
            "passWithNoTests": true,
            "scripts": [],
            "styles": [
              "node_modules/roboto-fontface/css/roboto/roboto-fontface.css",
              "node_modules/material-design-icon-fonts/iconfont/material-icons.css",
              "node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "apps/client/src/styles.scss"
            ]
          }
        }
      },
      "prefix": "client",
      "projectType": "application",
      "root": "apps/client",
      "schematics": {
        "@nrwl/workspace:component": {
          "style": "scss"
        }
      },
      "sourceRoot": "apps/client/src"
    },
    "client-e2e": {
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "configurations": {
            "production": {
              "devServerTarget": "client:serve:production"
            }
          },
          "options": {
            "cypressConfig": "apps/client-e2e/cypress.json",
            "devServerTarget": "client:serve",
            "tsConfig": "apps/client-e2e/tsconfig.e2e.json"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "apps/client-e2e/.eslintrc",
            "exclude": ["**/node_modules/**", "!apps/client-e2e/**"],
            "tsConfig": "apps/client-e2e/tsconfig.e2e.json"
          }
        }
      },
      "projectType": "application",
      "root": "apps/client-e2e",
      "sourceRoot": "apps/client-e2e/src"
    },
    "proto": {
      "architect": {
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "libs/proto/.eslintrc",
            "exclude": ["**/node_modules/**", "!libs/proto/**"],
            "tsConfig": ["libs/proto/tsconfig.lib.json", "libs/proto/tsconfig.spec.json"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/proto/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "prefix": "client",
      "projectType": "library",
      "root": "libs/proto",
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      },
      "sourceRoot": "libs/proto/src"
    },
    "client-core": {
      "architect": {
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "libs/client-core/.eslintrc",
            "exclude": ["**/node_modules/**", "!libs/client-core/**"],
            "tsConfig": [
              "libs/client-core/tsconfig.lib.json",
              "libs/client-core/tsconfig.spec.json"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/client-core/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "prefix": "client",
      "projectType": "library",
      "root": "libs/client-core",
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      },
      "sourceRoot": "libs/client-core/src"
    },
    "client-store": {
      "architect": {
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "libs/client-store/.eslintrc",
            "exclude": ["**/node_modules/**", "!libs/client-store/**/*"],
            "tsConfig": [
              "libs/client-store/tsconfig.lib.json",
              "libs/client-store/tsconfig.spec.json"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/client-store/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "prefix": "client",
      "projectType": "library",
      "root": "libs/client-store",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "sourceRoot": "libs/client-store/src"
    },
    "client-ui": {
      "architect": {
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "libs/client-ui/.eslintrc",
            "exclude": ["**/node_modules/**", "!libs/client-ui/**/*"],
            "tsConfig": ["libs/client-ui/tsconfig.lib.json", "libs/client-ui/tsconfig.spec.json"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/client-ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "prefix": "client",
      "projectType": "library",
      "root": "libs/client-ui",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "sourceRoot": "libs/client-ui/src"
    },
    "client-util": {
      "architect": {
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "libs/client-util/.eslintrc",
            "exclude": ["**/node_modules/**", "!libs/client-util/**/*"],
            "tsConfig": [
              "libs/client-util/tsconfig.lib.json",
              "libs/client-util/tsconfig.spec.json"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/client-util/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "prefix": "client",
      "projectType": "library",
      "root": "libs/client-util",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "sourceRoot": "libs/client-util/src"
    },
    "client-ui-material": {
      "projectType": "library",
      "root": "libs/client-ui-material",
      "sourceRoot": "libs/client-ui-material/src",
      "prefix": "client",
      "architect": {
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "libs/client-ui-material/.eslintrc",
            "tsConfig": [
              "libs/client-ui-material/tsconfig.lib.json",
              "libs/client-ui-material/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/client-ui-material/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/client-ui-material/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    },
    "client-ui-translate": {
      "projectType": "library",
      "root": "libs/client-ui-translate",
      "sourceRoot": "libs/client-ui-translate/src",
      "prefix": "client",
      "architect": {
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "libs/client-ui-translate/.eslintrc",
            "tsConfig": [
              "libs/client-ui-translate/tsconfig.lib.json",
              "libs/client-ui-translate/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/client-ui-translate/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
