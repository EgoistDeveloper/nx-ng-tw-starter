            "passWithNoTests": true
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    },
    "client-services": {
      "projectType": "library",
      "root": "libs/client-services",
      "sourceRoot": "libs/client-services/src",
      "prefix": "client",
      "architect": {
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "libs/client-services/.eslintrc",
            "tsConfig": [
              "libs/client-services/tsconfig.lib.json",
              "libs/client-services/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/client-services/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/client-services/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    }
  },
  "schematics": {
    "@nrwl/angular:application": {
      "e2eTestRunner": "cypress",
      "style": "scss",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "unitTestRunner": "jest"
    },
    "@nrwl/schematics:application": {
      "framework": "angular",
      "style": "scss"
    },
    "@nrwl/schematics:library": {
      "style": "scss",
      "unitTestRunner": "jest"
    }
  },
  "version": 1
}
