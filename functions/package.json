{
  "name": "nx-ng-starter-server",
  "private": true,
  "description": "Cloud Functions for Firebase",
  "main": "main.js",
  "scripts": {
    "clear-api-dist": "rm -f ./main.js ./main.js.map ./*.txt; rm -rf ./proto",
    "deploy": "firebase deploy --only functions",
    "get-api-dist": "npm run clear-api-dist; cp -r ../dist/apps/api/* ./; cp -r ../tools/proto ./proto",
    "logs": "firebase functions:log",
    "serve": "firebase serve --only functions",
    "shell": "firebase experimental:functions:shell",
    "start": "npm run shell",
    "start:emulators": "firebase emulators:start --only functions"
  },
  "dependencies": {
    "@grpc/proto-loader": "0.5.4",
    "@nestjs/common": "7.1.3",
    "@nestjs/core": "7.1.3",
    "@nestjs/graphql": "7.4.1",
    "@nestjs/jwt": "7.0.0",
    "@nestjs/microservices": "7.1.3",
    "@nestjs/passport": "7.0.0",
    "@nestjs/platform-express": "7.1.3",
    "apollo-server-express": "2.14.3",
    "class-transformer": "0.2.3",
    "class-validator": "0.12.2",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "firebase-admin": "8.12.1",
    "firebase-functions": "3.7.0",
    "google-protobuf": "3.12.2",
    "graphql": "15.1.0",
    "graphql-subscriptions": "1.1.0",
    "graphql-tag": "2.10.3",
    "graphql-tools": "6.0.9",
    "graphql-upload": "11.0.0",
    "grpc": "1.24.3",
    "grpc-web": "1.1.0",
    "jwt-simple": "0.5.6",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.5"
  },
  "engines": {
    "node": "10",
    "npm": ">=6.14.4"
  }
}
