{
  "angularCompilerOptions": {
    "annotateForClosureCompiler": true,
    "enableIvy": true,
    "enableResourceInlining": true,
    "fullTemplateTypeCheck": true,
    "skipTemplateCodegen": true,
    "strictInjectionParameters": true,
    "strictMetadataEmit": true,
    "strictTemplates": true
  },
  "compileOnSave": false,
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "declaration": false,
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "lib": ["ES2018", "DOM", "ESNext.AsyncIterable"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": false,
    "noUnusedLocals": true,
    "paths": {
      "@nx-ng-starter/api-interface": ["libs/api-interface/src/index.ts"],
      "@nx-ng-starter/api-interface/client-gql": ["libs/api-interface/src/lib/generated/index.ts"],
      "@nx-ng-starter/mocks-core": ["libs/mocks-core/src/index.ts"],
      "@nx-ng-starter/mocks-core/data-access": ["libs/mocks-core/src/lib/data-access/index.ts"],
      "@nx-ng-starter/mocks-core/ui": ["libs/mocks-core/src/lib/ui/index.ts"],
      "@nx-ng-starter/mocks-core/util": ["libs/mocks-core/src/lib/util/index.ts"],
      "@nx-ng-starter/proto": ["libs/proto/src/index.ts"],
      "@nx-ng-starter/shared-core": ["libs/shared-core/src/index.ts"],
      "@nx-ng-starter/shared-core/interfaces": ["libs/shared-core/src/lib/interfaces/index.ts"],
      "@nx-ng-starter/shared-core/services": ["libs/shared-core/src/lib/services/index.ts"],
      "@nx-ng-starter/shared-store": ["libs/shared-store/src/index.ts"],
      "@nx-ng-starter/shared-store/*": ["libs/shared-store/src/lib/*"],
      "@nx-ng-starter/shared-ui": ["libs/shared-ui/src/index.ts"],
      "@nx-ng-starter/shared-util": ["libs/shared-util/src/index.ts"]
    },
    "rootDir": ".",
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "ES6",
    "typeRoots": ["node_modules/@types"]
  },
  "exclude": [
    "node_modules",
    "tmp",
    ".azure",
    ".devops",
    ".firebase",
    ".vscode",
    "coverage",
    "dist",
    "documentation",
    "functions",
    "libs/proto/src/lib/grpc",
    "libs/proto/src/lib/ts",
    "tools/proto",
    "tools/shell"
  ]
}
